New FAQ
<%= form_for @faq do |f| %>
  <div class="form-group">
    <%= f.label :question %>
    <%= f.text_field :question %>
  </div>
  <div class="form-group">
    <%= f.label :answer %>
    <%= f.text_area :answer %>
  </div>
  <div class="form-group">
    <%= f.submit "Save FAQ" %>
  </div>
<% end %>
